<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="4" parallel="tests" name="FDM_UI_regression_Tests">
    <test name="Регистрация и авторизация" verbose="10">
        <classes>
            <class name="UI_Tests.test.RegLogNavFormTests.FdmRegistrationTest"/>
            <class name="UI_Tests.test.RegLogNavFormTests.FdmLoginTest"/>
        </classes>
    </test>
    <test name="Навигация на сайте и ссылки" verbose="10">
        <classes>
            <class name="UI_Tests.test.RegLogNavFormTests.FdmLinksTest"/>
            <class name="UI_Tests.test.RegLogNavFormTests.FdmNavigationTest"/>
        </classes>
    </test>
    <test name="Форма обратной связи" verbose="10">
        <classes>
            <class name="UI_Tests.test.RegLogNavFormTests.FdmFeedBackFormTest"/>
        </classes>
    </test>
    <test name="Проверка оформления заказа по компании Кедр" verbose="10">
        <classes>
            <class name="UI_Tests.test.KedrCompany.KedrFacadePlasticTest"/>
            <class name="UI_Tests.test.KedrCompany.KedrPlasticSelectsTest"/>
        </classes>
    </test>
    <test name="Проверка оформления заказа по компании Стандарт" verbose="10">
        <classes>
            <class name="UI_Tests.test.Standart.StandartFacadePvhTest"/>
        </classes>
    </test>

</suite>
